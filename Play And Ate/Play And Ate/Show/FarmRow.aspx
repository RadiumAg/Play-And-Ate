    function ORG7188TourInit() {
        if ($.browser.msie) {
            $('input:checkbox').click(function () {
                this.blur();
                this.focus();
            });
        }

        $('#leaveMsgPop').jqm();
        $('#submitMsg').click(function () {
            var o1 = $.trim($('#leaveMsg .p1 input.input1').val()); //手机
            var o2 = $.trim($('#leaveMsg textarea').val()); //留言内容
            var o3 = $.trim($('#leaveMsg .p2 input.input1').val()); //邮箱
            var place = $("#leaveMsg").attr("place"); //产品名称
            var pcode = $("#leaveMsg").attr("pcode"); //产品id

            var url = document.location; //产品URL
            var uname = $("#leaveMsg").attr("uname"); //用户名
            if (checkMsgValidate()) {
                //判断用户状态
                var userName = $('#weclome').find('b.blue');
                if (userName.text().length > 0) {
                    $('#leaveMsgPop').find('.d2').show();
                }
                else {
                    $('#leaveMsgPop').find('.d1').show();
                }

                $.ajax({
                    type: "get",
                    date: "json",
                    url: "http://xianlu.01nz.com.cn/add/postmsg.php?phone=" + o1 + "&content=" + escape(o2) + "&email=" + o3 + "&place=" + escape(place) + "&pcode=" + pcode + "&url=" + url + "&uname=" + escape(uname) + "&rad=" + Math.random(),
                    success: function (msg) {

                        if (msg == "success") {
                            $('#leaveMsgPop').jqmShow();
                            if ('' != "") {
                                $('#leaveMsg .p1 input.input1').val("");
                                $('#leaveMsg .p2 input.input1').val("");
                            }
                            else {
                                $('#leaveMsg .p1 input.input1').val("");
                                $('#leaveMsg .p2 input.input1').val("");
                            }
                            $('#leaveMsg textarea').val("");
                            $(".errorInfo").text("");
                        }
                        else if (msg == "much") {
                            alert("您的留言过于频繁，请稍后再试");
                        }
                        else {
                            alert("留言失败！" + msg + "");
                        }
                    }
                });
            }
        });


        $('#leaveMsg .p1 .s1 label input:checkbox').change(function () {
            var o = $(this);
            var oS = o.parents('p.p1').next("p.p2");
            if (o.attr('checked') == true) {
                oS.show();
            }
            else {
                oS.hide();
            }
        });


    }

    function checkMsgValidate() {

        var ck = $('#leaveMsg').find('input:checkbox');

        var o1 = $.trim($('#leaveMsg .p1 input.input1').val()); //手机
        var o2 = $.trim($('#leaveMsg textarea').val()); //留言内容
        var o3 = $.trim($('#leaveMsg .p2 input.input1').val()); //邮箱


        var regexMobile = /^13[0-9]{1}[0-9]{8}$|^15[9]{1}[0-9]{8}$|^1[3,5]{1}[0-9]{1}[0-9]{8}$|^18[0-9][0-9]{8}$/; //手机
        var regexEmail = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/; //邮箱

        if (o1.length < 1) {
            $(".errorInfo").text("(*)请输入手机号");
            return false;
        }

        if (!regexMobile.test(o1)) {
            $(".errorInfo").text("(*)请输入正确的手机号");
            return false;
        }

        if (o2.length < 1) {
            $(".errorInfo").text("(*)请输入留言内容");
            return false;
        }

        if (o3.length < 1) {
            if (ck.attr("checked") == true) {
                $(".errorInfo").text("(*)请输入您的邮箱");
                return false;
            }
        }

        else if (!regexEmail.test(o3)) {
            if (ck.attr("checked") == true) {
                $(".errorInfo").text("(*)请输入正确的邮箱");
                return false;
            }
        }
        return true;

    }
    
 
